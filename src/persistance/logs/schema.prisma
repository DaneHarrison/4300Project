generator client {
    provider             = "prisma-client-py"
    interface            = "sync"
    recursive_type_depth = 5
    output               = "./DB"
}

datasource db {
    provider = "postgresql"
    url      = env("DB_LOG_URL")
}

model Entry {
    entryID     Int     @id @default(autoincrement())
    entryPhoto  String  @unique
}
