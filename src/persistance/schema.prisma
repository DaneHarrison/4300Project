generator client {
  provider = "prisma-client-js"
}

datasource db {
	provider = "postgresql"
  url      = env("DB_URL")
}

model User {
  ID 			  Int		@id @default(autoincrement())
  Name      String
  Weight    Int[]
  Photo     Bytes[]
}